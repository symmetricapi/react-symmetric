{
  "name": "symmetric-react",
  "version": "1.0.0",
  "description": "React hooks for symmetric-js",
  "keywords": [
    "react",
    "observable",
    "model",
    "collection",
    "rest",
    "api",
    "fetch",
    "hooks",
    "symmetric"
  ],
  "main": "dist/symmetric-react.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "release": "yarn build && git tag $npm_package_version && git push --tags && yarn publish --no-lockfile",
    "test": "node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:symmetricapi/symmetric-react.git"
  },
  "homepage": "https://github.com/symmetricapi/symmetric-react",
  "bugs": {
    "url": "https://github.com/symmetricapi/symmetric-react/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.15.1",
    "eslint-plugin-import": "^2.16.0",
    "prettier": "^1.16.4",
    "rollup": "^2.10.7",
    "rollup-plugin-babel": "^4.4.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "symmetric-js": "^1.0.15"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "react": "^16.13.1",
    "symmetric-js": "^1.0.15"
  }
